{
  "name": "notiwind-ts",
  "description": "A headless Vue 3 notification library to use with Tailwind CSS. TypeScript Edition",
  "version": "2.0.2",
  "main": "dist/notiwind-ts.umd.js",
  "module": "dist/notiwind-ts.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "dist/notiwind-ts.es.js",
    "require": "dist/notiwind-ts.umd.js"
  },
  "license": "MIT",
  "author": {
    "name": "Markus Fl√ºr",
    "url": "https://github.com/lacrioque"
  },
  "homepage": "https://github.com/lacrioque/notiwind-ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:lacrioque/notiwind-ts.git"
  },
  "bugs": {
    "url": "https://github.com/lacrioque/notiwind-ts/issues"
  },
  "scripts": {
    "build": "rm -rf dist/ && vite build --mode=production && vue-tsc --emitDeclarationOnly",
    "dev": "rm -rf dist/ && vite build --mode=development",
    "lint": "eslint --ext .ts,.vue src --ignore-pattern *.d.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@rollup/plugin-typescript": "^8.5.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.17.0",
    "eslint-plugin-vue": "^9.1.0",
    "mitt": "^3.0.0",
    "rollup": "^2.75.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vue": "^3.2.39",
    "vue-tsc": "^0.40.13"
  },
  "peerDependencies": {
    "vue": "^3.2.37"
  },
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "vuejs",
    "tailwindcss",
    "tailwindui",
    "notifications",
    "toast",
    "typescript"
  ],
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "dist/",
    "src/"
  ]
}
